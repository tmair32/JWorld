{
  "name": "jworld",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write './**/*.{vue,js,ts}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^9.1.0",
    "@vueuse/head": "^0.7.9",
    "prism-theme-vars": "^0.2.4",
    "vite-ssg": "^0.20.2",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@babylonjs/core": "^5.19.0",
    "@babylonjs/gui": "^5.19.0",
    "@babylonjs/loaders": "^5.19.0",
    "@types/node": "^18.7.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@windicss/plugin-question-mark": "^0.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "gray-matter": "^4.0.3",
    "husky": ">=7",
    "lint-staged": ">=10",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.4",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-prism": "^2.2.4",
    "markdown-it-table-of-contents": "^0.6.0",
    "prettier": "^2.7.1",
    "prismjs": "^1.28.0",
    "sass": "^1.54.4",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.11.1",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.0.7",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-md": "^0.20.2",
    "vite-plugin-pages": "^0.25.0",
    "vite-plugin-windicss": "^1.8.7",
    "vue-tsc": "^0.39.5",
    "windicss": "^3.5.6"
  },
  "lint-staged": {
    "**/*.{js,vue,ts}": [
      "prettier . --write",
      "eslint --ext .vue,.ts,.js --ignore-path .gitignore --fix src"
    ],
    "*.{js,css,scss}": "prettier --write"
  }
}
